# Environment Configuration
# Copy this file to .env and update values as needed

# Backend Configuration
API_HOST=0.0.0.0
API_PORT=8000
LOG_LEVEL=INFO

# CORS Configuration (comma-separated origins)
CORS_ORIGINS=*

# Model Paths
MODEL_DIR=./models
DATA_DIR=./data

# Feature Flags
ENABLE_VISUAL_ANALYSIS=true
ENABLE_ML_MODEL=true
